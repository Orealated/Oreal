if getgenv().ActivateTheUltimateTrolly then

game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "[ New Loadstring Copied ]",
	Text = "Please Use The New Loadstring Copied in Your Clipboard",
	Duration = math.huge
})

local script = [[
--// Read This First Before Executing

New Loadstring:
getgenv().ActivateTheUltimateTrolly = true
local a, b, e = loadstring, request or http_request or (http and http.request), "https://raw.githubusercontent.com/Orealated/Oreal/main/orealated%20Easy.gg%20Ultimate%20Trolling%20Hub%20Loader"
if not (a and b) then game.Players.LocalPlayer:Kick("[( Executor not Supported )] - Please Use an Executor Like Delta, Fluxus or Arceus X Neo.") else 
a(game:HttpGet(e, true))(); end

--// If Your Executor Isn't Supported Then That Means
Some of The Scripts Inside The Hub Will Not Work
For Your Executor

--// Use This if it Isnt Working:
getgenv().ActivateTheUltimateTrolly = true
loadstring(game:HttpGet("https://raw.githubusercontent.com/Orealated/Oreal/main/orealated%20Easy.gg%20Ultimate%20Trolling%20Hub%20Loader", true))();]]

setclipboard(script)

else
    local bombasitcsideeye =
        [[getgenv().ActivateTheUltimateTrolly = true
loadstring(game:HttpGet("https://raw.githubusercontent.com/Orealated/Oreal/main/orealated%20Easy.gg%20UTH%20Loader", true))();]]
    setclipboard(bombasitcsideeye)
    game:GetService("StarterGui"):SetCore(
        "SendNotification",
        {
            Title = "[ New Loadstring Copied ]",
            Text = ":] Please Use The New Loadstring to Continue Using my Hub :]",
            Duration = 20
        }
    )
end
